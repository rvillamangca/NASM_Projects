    global  main
    extern  printf

    section .data
value:  dd   5
name:   db  "Problem 01",0
fmt:    db  "%s Answer:",9,"%d",10,0

    section .text  
main:
    enter   0,0
    
    push    dword [value]
    push    name
    push    fmt
    call    printf
    
    leave
    xor     eax, eax
    ret